STATS			START			0
			EXTREF			XREAD,XWRITE
			STL			RETADR
			J			{EXADDR}
RETADR			RESW			1
SUM			RESW			1
SUMSQ			RESW			1
I			RESW			1
VALUE			RESW			1
MEAN			RESW			1
VARIANCE			RESW			1
{EXADDR}			LDA			#0
			STA			SUM
			LDA			#0
			STA			SUMSQ
			LDA			#1
L1			STA			I
			COMP			#100
			JGT			L2
			+JSUB			XREAD
			WORD			1
			WORD			VALUE
			LDA			SUM
			ADD			VALUE
			STA			SUM
			LDA			VALUE
			MUL			VALUE
			ADD			SUMSQ
			STA			SUMSQ
			LDA			I
			ADD			#1
			J			L1
L2			+JSUB			XWRITE
			WORD			2
			WORD			MEAN
			WORD			VARIANCE
			LDL			RETADR
			RSUB
			END
